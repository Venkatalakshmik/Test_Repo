<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="ExtractVariables-AuthHeader">
    <DisplayName>ExtractVariables-AuthHeader</DisplayName>
    <Properties/>
    <!-- <URIPath name="name"/>
    <QueryParam name="name"/> -->
    <Header name="Authorization">
        <Pattern ignoreCase="false">bearer {AuthHeader}</Pattern>
    </Header>
    <!--<FormParam name="name"/> -->
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">request</Source>
    <VariablePrefix>orders</VariablePrefix>
</ExtractVariables>